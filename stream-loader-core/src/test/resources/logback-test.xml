<configuration>

    <appender name="STDOUT" class="ch.qos.logback.core.ConsoleAppender">
        <encoder>
            <pattern>[%d] %-5level %msg \(%c\)%n</pattern>
        </encoder>
    </appender>

    <logger name="com.adform.streamloader.util" level="OFF" />

    <root level="ERROR">
        <appender-ref ref="STDOUT" />
    </root>

</configuration>
